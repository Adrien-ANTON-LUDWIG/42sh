name: simple_if
command: ../build/42sh -c "if echo foo; then echo bar; fi"
stdout: |
  WORD_IF
  WORD_COMMAND echo foo
  WORD_THEN
  WORD_COMMAND echo bar
  WORD_FI
  WORD_EOF
returncode: 0
---
name: redir_stdout_trunc
command: ../build/42sh -c "echo foo > file"
stdout: |
  WORD_COMMAND echo foo
  WORD_REDIR 1> file
  WORD_EOF
returncode: 0
---
name: redir_stdout_append
command: ../build/42sh -c "echo foo >> file"
stdout: |
  WORD_COMMAND echo foo
  WORD_REDIR 1>> file
  WORD_EOF
returncode: 0
---
name: redir_stderr_trunc
command: ../build/42sh -c "echo foo 2> file"
stdout: |
  WORD_COMMAND echo foo
  WORD_REDIR 2> file
  WORD_EOF
returncode: 0
---
name: redir_stderr_append
command: ../build/42sh -c "echo foo 2>> file"
stdout: |
  WORD_COMMAND echo foo
  WORD_REDIR 2>> file
  WORD_EOF
returncode: 0
---
name: redir_stdout_append_stderr_trunc
command: ../build/42sh -c "echo foo 1>> out 2> err"
stdout: |
  WORD_COMMAND echo foo
  WORD_REDIR 1>> out 2> err
  WORD_EOF
returncode: 0
---
name: redir_stdin
command: ../build/42sh -c "cat < file"
stdout: |
  WORD_COMMAND cat
  WORD_REDIR < file
  WORD_EOF
returncode: 0