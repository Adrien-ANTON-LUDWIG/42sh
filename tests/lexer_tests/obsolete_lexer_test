name: empty_string
command: ../build/42sh -c ""
stdout: |
  WORD_EOF
returncode: 0
---
name: easy_command
command: ../build/42sh -c "echo simple"
stdout: |
  WORD_COMMAND echo simple
  WORD_EOF
returncode: 0
---
name: normal_command
command: ../build/42sh -c "echo toto tata"
stdout: |
  WORD_COMMAND echo toto tata
  WORD_EOF
returncode: 0
---
name: two_commands_;
command: ../build/42sh -c "echo foo; echo bar"
stdout: |
  WORD_COMMAND echo foo
  WORD_COMMAND echo bar
  WORD_EOF
returncode: 0
